{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "plugins": ["react-hooks"],
  "rules": {
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "arrow-body-style": [
      0,
      "never"
    ],
    "max-len": [2, 140],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "object-curly-newline": [
      "error",
      {
        "ImportDeclaration": {
          "multiline": true
        }
      }
    ],
    // "global-require": 0,
    // "func-names": 0,
    // "no-shadow": 1,
    "consistent-return": 0,
    // "prefer-const": 0,
    // "import/no-unresolved": 0,
    // "import/extensions": 0,
    // "max-len": 0,
    "no-param-reassign": 0
    // "no-unused-expressions": [
