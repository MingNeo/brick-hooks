{
  "name": "brick-hooks-root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "react/react-native通用hooks",
  "scripts": {
    "test": "pnpm jest",
    "init": "rm -rf node_modules && npm install && npm run clean && npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "pnpmBuild": "pnpm --filter './packages/**' build",
    "clean": "lerna clean --yes",
    "build": "lerna run build",
    "publish": "lerna publish",
    "prepublish": "lerna run build",
    "pull:hydrogen-store": "git subtree pull --prefix=packages/hydrogen-store hydrogen master",
    "push:hydrogen-store": "git subtree push --prefix=packages/hydrogen-store hydrogen master",
    "pull:h-store-effect-plugin": "git subtree pull --prefix=packages/hydrogen-store-effect-plugin effect-plugin master",
    "push:h-store-effect-plugin": "git subtree push --prefix=packages/hydrogen-store-effect-plugin effect-plugin master",
    "pull:h-store-redux-plugin": "git subtree pull --prefix=packages/hydrogen-store-redux-plugin git@github.com:MingNeo/hydrogen-store-redux-plugin.git master",
    "push:h-store-redux-plugin": "git subtree push --prefix=packages/hydrogen-store-redux-plugin git@github.com:MingNeo/hydrogen-store-redux-plugin.git master",
    "pull:h-store-immer-plugin": "git subtree pull --prefix=packages/hydrogen-store-immer-plugin git@github.com:MingNeo/hydrogen-store-immer-plugin.git master",
    "push:h-store-immer-plugin": "git subtree push --prefix=packages/hydrogen-store-immer-plugin git@github.com:MingNeo/hydrogen-store-immer-plugin.git master",
    "docs:start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "umi-test": "umi-test",
    "umi-test:coverage": "umi-test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MingNeo/custom-hooks.git"
  },
  "author": "lmr",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/core": "^3.5.0",
    "moment-business": "^3.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "^0.70.4",
    "react-router": "^6.4.3",
    "redux": "^4.2.0",
    "immer": "^9.0.3",
    "typescript": "^3.9.9",
    "umi": "3.5.0",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/jest": "^25.2.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^9.0.1",
    "babel-plugin-import": "^1.13.0",
    "create-test-server": "^3.0.1",
    "dayjs": "^1.10.4",
    "del": "^5.1.0",
    "dumi": "^1.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.23.0",
    "eslint-config-jest-enzyme": "^7.1.2",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-lerna": "^2.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "jest": "^26.4.2",
    "lerna": "^6.0.1",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "@react-native-community/async-storage": "^1.12.1"
  },
  "bugs": {
    "url": "https://github.com/MingNeo/custom-hooks/issues"
  },
  "homepage": "https://github.com/MingNeo/custom-hooks#readme",
  "license": "ISC",
  "version": "1.0.0",
  "main": ".eslintrc.js"
}
